{
    "version": 2,
    "builds": [
      {
        "src": "src/app.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "requirementsPath": "src/requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.py"
      }
    ]
  }